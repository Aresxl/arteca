---
const { title, description } = Astro.props;
---

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width" />
  <link
    rel="preload"
    href="/fonts/orbitron-v35-latin-900.woff2"
    as="font"
    type="font/woff2"
    crossorigin
  />
  <link
    rel="preload"
    href="/fonts/orbitron-v35-latin-700.woff2"
    as="font"
    type="font/woff2"
    crossorigin
  />
  <link
    rel="preload"
    href="/fonts/orbitron-v35-latin-regular.woff2"
    as="font"
    type="font/woff2"
    crossorigin
  />
  <link
    rel="preload"
    href="/fonts/jetbrains-mono-v24-latin-regular.woff2"
    as="font"
    type="font/woff2"
    crossorigin
  />
  <meta name="description" content={description} />
  <meta name="generator" content={Astro.generator} />
  <title>{title}</title>
</head>
